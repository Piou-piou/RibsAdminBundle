{% extends page.template %}

{% block title %}{{ page.titleTag }}{% endblock %}

{% block description %}{{ page.descriptionTag }}{% endblock %}

{% block ribsNavigation %}
	{% for i in navigation %}
		<li><a href="/page/{{ i.url }}" title="{{ i.title_tag }}">{{ i.title }}</a></li>
	{% endfor %}
{% endblock %}

{% block ribsBlock %}
	{{ page.content|raw }}
{% endblock %}