{% extends "@RibsAdmin/base.html.twig" %}

{% block body %}
<div class="ribs-container-fluid">
    <div class="top-links">
        <a href="{{ path('ribsadmin_packages') }}" class="ribs-button">Return</a>
    </div>

    <div class="page-title">
        <span>Packages</span>
        <h3>Show package update logs</h3>
    </div>

    <div class="row">
        <div class="cxs-12">
            <div class="block">
                <header>
                    <h5>Logs informations</h5>
                </header>

                <div class="row">
                    <div class="cxs-12">
                        {{ logs|raw }}
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
